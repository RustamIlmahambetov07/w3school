/* Use int when you need to store a whole number without decimals, like 35 or 1000, and float or double when you need a floating point number (with decimals), like 9.99 or 3.14515. */

//example 

#include <stdio.h>

int main() {
  int myNum = 1000;
  printf("%d", myNum);
  return 0;
}

#include <stdio.h>

int main() {
  float myNum = 1000;
  printf("%f", myNum);
  return 0;
}

#include <stdio.h>

int main() {
  double myNum = 1000;
  printf("%lf", myNum);
  return 0;
}